#! /bin/sh
case "$1" in
       purge|remove)
        dpkg-divert --package hotplug-kanotix --remove --rename \
		--divert /etc/hotplug/ieee1394.agent.kanotix /etc/hotplug/ieee1394.agent
        ;;
esac

# dh_installdeb will replace this with shell code automatically
# generated by other debhelper scripts.

#DEBHELPER#

exit 0
